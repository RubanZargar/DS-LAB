#include <stdio.h>
#define SIZE 100

int q1[SIZE], q2[SIZE];
int f1 = 0, r1 = -1;
int f2 = 0, r2 = -1;

// add element in queue
void enqueue(int q[], int *rear, int value) {
    q[++(*rear)] = value;
}

// remove element from queue
int dequeue(int q[], int *front) {
    return q[(*front)++];
}

// push operation of stack
void push(int value) {
    // step 1: push new element in empty queue q2
    enqueue(q2, &r2, value);

    // step 2: move all elements of q1 to q2
    while (f1 <= r1) {
        enqueue(q2, &r2, dequeue(q1, &f1));
    }

    // step 3: swap q1 and q2
    int temp;
    temp = f1; f1 = f2; f2 = temp;
    temp = r1; r1 = r2; r2 = temp;
}

// pop operation of stack
void pop() {
    if (f1 > r1) {
        printf("Stack is empty\n");
    } else {
        printf("Popped element: %d\n", dequeue(q1, &f1));
    }
}

int main() {
    push(10);
    push(20);
    push(30);

    pop();
    pop();

    return 0;
}